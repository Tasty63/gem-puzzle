{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": [
      "error",
      {
        "AssignmentExpression": {
          "array": false,
          "object": false
        }
      }
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "no-tabs": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}